# A Daq message contains parameters on which daq-commands should be sent to the UcBoard.
# All values are default false.

# Create a daq group
bool create_group 0

# Delete the daq group
bool delete_group 0

# Activate the daq group
bool activate 0

# Deactivate the daq group
bool deactivate 0

# Start the data acquisition
bool start 0

# Stop the data acquisition
bool stop 0
